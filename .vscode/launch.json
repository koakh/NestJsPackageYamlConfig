{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Program",
      "port": 9229,
      "request": "attach",
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/nestjs-package-starter-consumer/src/main.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    }
  ]
}